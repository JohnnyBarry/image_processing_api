{
  "name": "image_processing_api",
  "version": "1.0.0",
  "description": "API to return images of the desired size to the user",
  "main": "index.js",
  "scripts": {
    "lintjs": "eslint . --ext .js",
    "lintts": "eslint . --ext .ts",
    "prettierjs": "prettier --config .prettierrc \"src/**/*.js\" --write",
    "prettierts": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "build": "npx tsc",
    "jasmine": "jasmine",
    "test": "npm run build && npm run jasmine",
    "start": "npx nodemon src/index.ts",
    "serve": "npm run build && node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohnnyBarry/image_processing_api.git"
  },
  "keywords": [
    "image",
    "processing"
  ],
  "author": "JohnnyBarry",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JohnnyBarry/image_processing_api/issues"
  },
  "homepage": "https://github.com/JohnnyBarry/image_processing_api#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.2",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@types/supertest": "^2.0.11",
    "express": "^4.17.1",
    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "supertest": "^6.1.6"
  }
